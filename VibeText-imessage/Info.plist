<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>NSMicrophoneUsageDescription</key>
	<string>VibeText needs microphone access to record your voice messages.</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>VibeText uses speech recognition to transcribe your voice into text.</string>
	<key>NSExtension</key>
	<dict>
		<key>NSExtensionPrincipalClass</key>
		<string>$(PRODUCT_MODULE_NAME).MessagesViewController</string>
		<key>NSExtensionPointIdentifier</key>
		<string>com.apple.message-payload-provider</string>
		<key>NSExtensionAttributes</key>
		<dict>
			<key>IntentsSupported</key>
			<array/>
			<key>IntentsRestrictedWhileLocked</key>
			<array/>
		</dict>
	</dict>
</dict>
</plist>
